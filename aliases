unalias -a

alias ip="curl ipinfo.io"
alias weather="curl wttr.in"
alias '?'=duck
alias '??'=google
alias '???'=bing
alias g="git"
alias a="git add"
alias c="git commit"
alias d="git diff"
alias s="git status"

alias td="grep -r \"\- \[ \]\""
alias tdall="grep -riE '(\- \[ \]|\- \[X\])'"
alias tdone="grep -ri \"\- \[X\]\""
